views:
  - type: table
    name: Table
    filters:
      and:
        - file.inFolder("Internal/Content Ideas")
        - '!file.name.contains("Base")'
        - Status != "Done"
    order:
      - file.name
      - Status
      - file.mtime
      - tags
    sort:
      - property: file.mtime
        direction: DESC
    columnSize:
      file.name: 449
      note.Status: 156
