views:
  - type: table
    name: Table
    filters:
      and:
        - file.inFolder("Internal/Social Media Content Ideas")
        - '!file.name.contains("Base")'
    order:
      - file.name
      - Status
      - file.mtime
    sort:
      - property: file.mtime
        direction: DESC
    columnSize:
      file.name: 449
      note.Status: 156
